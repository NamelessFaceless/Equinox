{
  "name": "equinox",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-starter.js",
  "homepage": "./",
  "dependencies": {
    "antd": "^3.10.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "scripts": {
    "react-start": "set BROWSER=none && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && wait-on http://localhost:3000 && electron .",
    "start": "npm-run-all --parallel react-start electron-dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.0",
    "electron": "^3.0.6",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.1.0",
    "wait-on": "^3.1.0"
  }
}
